<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!--实体类所在的包名-->
    <typeAliases>
        <!--不是实体类的包名也可以-->
        <package name="com.underworld.domain"/>
    </typeAliases>
    <!--所有xml配置文件-->
    <mappers>
        <!--mapper文件名称和dao接口名必须完全一致,包括大小写-->
        <package name="com.underworld.dao"/>
    </mappers>

</configuration>